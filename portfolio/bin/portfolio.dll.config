<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.web>
		<!-- Set authentication mode, session state, and compilation settings -->
		<authentication mode="Forms" />
		<compilation debug="true" targetFramework="4.8" />
		<customErrors mode="Off" />
		<sessionState mode="InProc" timeout="20" />
	</system.web>

	<system.webServer>
		<!-- IIS settings -->
		<defaultDocument>
			<files>
				<add value="Default.aspx" />
			</files>
		</defaultDocument>
		<directoryBrowse enabled="false" />
	</system.webServer>

	<!-- Empty connectionStrings section -->
	<connectionStrings>
		<!-- Add your connection strings here when needed -->
	</connectionStrings>

	<appSettings>
		<!-- Add application settings here -->
	</appSettings>
</configuration>
